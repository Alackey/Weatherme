{
  "name": "expressgen",
  "version": "0.0.1",
  "private": true,
  "main": "./server/app.js",
  "scripts": {
    "prestart": "babel server -d build",
    "start": "node ./build/bin/www.js",
    "dev": "nodemon --ignore 'build' --exec npm start",
    "test": "mocha --recursive test",
    "test:coverage": "nyc npm test",
    "test:unit": "mocha --recursive test/middleware test/models test/routes",
    "test:integration": "mocha --recursive test/integration"
  },
  "dependencies": {
    "aws-sdk": "^2.22.0",
    "body-parser": "^1.13.3",
    "cookie-parser": "^1.3.3",
    "express": "^4.13.3",
    "morgan": "^1.6.1",
    "nunjucks": "^3.0.0",
    "serve-favicon": "^2.3.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "chai": "^3.5.0",
    "debug": "^2.2.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "supertest": "^2.0.0"
  }
}
